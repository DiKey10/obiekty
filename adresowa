class ksiazka_adresowa:
  def __init__(self,imie, nazwisko, firma, stanowisko, mail):
    self.imie = imie
    self.nazwisko = nazwisko
    self.firma = firma
    self.stanowisko = stanowisko
    self.mail = mail

pierwszy = ksiazka_adresowa(imie="Aleksander",nazwisko="Kowalski",firma="Listenin",stanowisko="Mathematician",mail="AleksanderKowalski@armyspy.com")
